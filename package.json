{
  "name": "svgdom-ts",
  "version": "1.0.0",
  "description": "SVG DOM Implementation",
  "main": "dist/index.js",
  "scripts": {
    "test": "tap test/parse.mjs",
    "madge": "node test/madge.cjs",
    "test_xml": "python test/gen.py > test/test.xml",
    "rm-map": "find dist -name '*.js.map' -type f -delete",
    "c8test": "c8 npx tap --no-coverage --reporter=terse 'test/**/*.tap.?js'",
    "wpt": "c8 npx tap --no-coverage --reporter=terse 'test/wpt/**/*.tap.?js'",
    "c8report": "c8 report -r text-lcov > coverage/lcov.info",
    "c8rephtml": "c8 report -r html",
    "html:switch": "env SWITCH=html node test/elements.js",
    "html:class": "env CLASS=html node test/elements.js",
    "html:import": "env IMPORT=html node test/elements.js",
    "compile": "tsc --sourcemap"
  },
  "author": "",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "license": "ISC",
  "dependencies": {
    "@types/sax": "^1.2.3",
    "css-select": "^4.1.3",
    "image-size": "",
    "saxes": "^5.0.1"
  },
  "c8": {
    "reports-dir": "/tmp/.c8/rep",
    "temp-directory": "/tmp/.c8/tmp",
    "src": "dist"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "jsdom": "^16.6.0",
    "mocha-sugar-free": "^1.4.0",
    "source-map-support": "^0.5.19",
    "tap": "^15.0.9"
  }
}

{
  "name": "svgdom-ts",
  "version": "1.0.0",
  "description": "SVG DOM Implementation",
  "main": "index.js",
  "scripts": {
    "test": "tap test/parse.mjs",
    "test+cov": "tap --coverage test/parse.mjs",
    "posttest": "tap --coverage-report=text-lcov | codecov",
    "rm-map": "find dist -name '*.js.map' -type f -delete",
    "c8test": "c8 npx tap --no-coverage test/*.tap.*js",
    "c8report": "c8 report -r text-lcov > coverage/lcov.info",
    "compile": "tsc -p . --sourcemap"
  },
  "author": "",
  "type": "module",
  "license": "ISC",
  "dependencies": {
    "@types/sax": "^1.2.3",
    "image-size": "",
    "saxes": "^5.0.1"
  },
  "c8": {
    "reports-dir": "/tmp/.c8/rep",
    "temp-directory": "/tmp/.c8/tmp",
    "src": "dist"
  },
  "devDependencies": {
    "jsdom": "^16.6.0",
    "source-map-support": "^0.5.19",
    "tap": "^15.0.9"
  }
}
